{
    "name": "mrkatz/svgicon",
    "description": "Laravel package to preload cached icons into blade views",
    "version": "1.0.0",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "~3.6.7 || ~3.7.8 || ~3.8.6 || ^4.8 || ^5.2 || ^6.0 || ^9.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "pestphp/pest-plugin-faker": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Kaczocha",
            "email": "adamkaczocha@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mrkatz\\SvgIcons\\": "src/"
        },
        "files": [
            "Mrkatz/SvgIcons/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mrkatz\\SvgIcons\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrkatz\\SvgIcons\\SvgIconsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor\\bin\\pest"
    }
}